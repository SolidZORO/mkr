{
  "name": "mkr",
  "description": "make React project with cra/craco and typescript",
  "version": "0.1.1",
  "license": "MIT",
  "author": "Jason Feng <solidzoro@live.com>",
  "private": true,
  "scripts": {
    "-------------------------------- LINT ----": "----",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "fix": "yarn run lint-js --fix",
    "tsc-check": "tsc -v && tsc --noEmit",
    "format": "prettier --write src/**/*.ts{,x}",
    "-------------------------------- TOOL ----": "----",
    "versionup": "standard-version -r patch",
    "versionup:minor": "standard-version -r minor",
    "versionup:major": "standard-version -r major",
    "versionup:init": "standard-version --first-release",
    "eject": "react-scripts eject",
    "-------------------------------- DEV  ----": "----",
    "dev": "yarn start",
    "start": "craco start",
    "test": "craco test",
    "analyze": "yarn build --analyze",
    "serve": "yarn build && serve -s build",
    "-------------------------------- PROD ----": "----",
    "build": "craco build"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.11.2",
    "classnames": "^2.2.6",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-icons": "^4.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@commitlint/config-conventional": "^11.0.0",
    "@craco/craco": "^6.1.0",
    "@types/classnames": "^2.2.11",
    "@types/eslint": "^7.2.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/loadable__component": "^5.13.1",
    "@types/prettier": "^2.1.6",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-import": "^1.13.3",
    "commitlint": "^11.0.0",
    "craco-alias": "^2.1.1",
    "craco-less": "^1.17.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.1.2",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
