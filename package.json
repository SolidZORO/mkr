{
  "name": "mkr",
  "description": "make React project with cra/craco and typescript",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Jason Feng <solidzoro@live.com>",
  "private": true,
  "scripts": {
    "-------------------------------- LINT ----": "----",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "fix": "yarn run lint --fix",
    "tsc-check": "tsc -v && tsc --noEmit",
    "format": "prettier --write src/**/*.ts{,x}",
    "-------------------------------- TOOL ----": "----",
    "versionup": "standard-version -r patch",
    "versionup:minor": "standard-version -r minor",
    "versionup:major": "standard-version -r major",
    "versionup:init": "standard-version --first-release",
    "eject": "react-scripts eject",
    "-------------------------------- DEV  ----": "----",
    "dev": "yarn start",
    "start": "craco start",
    "test": "craco test",
    "analyze": "yarn build --analyze",
    "serve": "yarn build && serve -s build",
    "-------------------------------- PROD ----": "----",
    "build": "craco build"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "antd": "^4.15.0",
    "classnames": "^2.3.1",
    "clone": "^2.1.2",
    "history": "^5.0.0",
    "next-plugin-antd-less": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-icons": "^4.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@commitlint/config-conventional": "^12.1.1",
    "@craco/craco": "^6.1.1",
    "@types/classnames": "^2.2.11",
    "@types/eslint": "^7.2.8",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/loadable__component": "^5.13.3",
    "@types/prettier": "^2.2.3",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-plugin-import": "^1.13.3",
    "commitlint": "^12.1.1",
    "craco-alias": "^2.2.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "module-resolver": "^1.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.3",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
