{
  "name": "mkr",
  "description": "make React project with cra/craco and typescript",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Jason Feng <solidzoro@live.com>",
  "private": true,
  "scripts": {
    "-------------------------------- LINT ----": "----",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "fix": "yarn run lint --fix",
    "tsc-check": "tsc -v && tsc --noEmit",
    "format": "prettier --write src/**/*.ts{,x}",
    "prepare": "husky install",
    "-------------------------------- TOOL ----": "----",
    "versionup": "standard-version -r patch",
    "versionup:minor": "standard-version -r minor",
    "versionup:major": "standard-version -r major",
    "versionup:init": "standard-version --first-release",
    "eject": "react-scripts eject",
    "-------------------------------- DEV  ----": "----",
    "dev": "yarn start",
    "start": "craco start",
    "test": "craco test",
    "analyze": "yarn build --analyze",
    "serve": "yarn build && serve -s build",
    "-------------------------------- PROD ----": "----",
    "build": "craco build"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "antd": "^4.16.3",
    "classnames": "^2.3.1",
    "clone": "^2.1.2",
    "history": "^5.0.0",
    "next-plugin-antd-less": "^1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-icons": "^4.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "web-vitals": "^2.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@commitlint/config-conventional": "^12.1.4",
    "@craco/craco": "^6.1.2",
    "@types/classnames": "^2.3.1",
    "@types/eslint": "^7.2.13",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/loadable__component": "^5.13.3",
    "@types/prettier": "^2.3.0",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-plugin-import": "^1.13.3",
    "commitlint": "^12.1.4",
    "craco-alias": "^3.0.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "module-resolver": "^1.0.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.3.2",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
